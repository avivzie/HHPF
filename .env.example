# API Configuration
# Copy this file to .env and fill in your actual API keys

# Together AI API Key (recommended for Llama-3 access)
# Get your key from: https://api.together.xyz/settings/api-keys
TOGETHER_API_KEY=your_together_api_key_here

# Groq API Key (alternative for fast inference)
# Get your key from: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# OpenAI API Key (optional fallback)
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
LLAMA_MODEL=meta-llama/Llama-3-8b-chat-hf  # or meta-llama/Llama-3-70b-chat-hf
DEFAULT_TEMPERATURE=0.8
MAX_TOKENS=1024
NUM_STOCHASTIC_SAMPLES=10

# Cache Configuration
CACHE_DIR=./cache
ENABLE_RESPONSE_CACHE=true
